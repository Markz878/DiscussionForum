{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.EntityFrameworkCore.Model.Validation": "Error",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "FileStorageSettings": {
    "ContainerName": "files",
    "StorageUri": "http://127.0.0.1:10000/devstoreaccount1"
  },
  "ConnectionStrings": {
    "SqlServer": "Data Source=127.0.0.1,1433;Initial Catalog=DiscussionForum;User ID=sa;Password=yourStrong(!)Password;Connect Timeout=30;Encrypt=False;Trust Server Certificate=False;Application Intent=ReadWrite;Multi Subnet Failover=False;"
  },
  "AddLogging": true,
  "RateLimitOptions": {
    "AnonTokenLimit": 10,
    "AnonQueueLimit": 10,
    "AnonReplenishmentPeriod": 1,
    "AnonTokensPerPeriod": 10,
    "UserTokenLimit": 5,
    "UserQueueLimit": 5,
    "UserReplenishmentPeriod": 1,
    "UserTokensPerPeriod": 5
  },
  "AllowedHosts": "*"
}
